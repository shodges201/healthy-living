{
  "name": "healthy-living",
  "version": "1.0.0",
  "description": "Live Healthier",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd ../client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shodges201/healthy-living.git"
  },
  "author": "Scott Hodges",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.16.3",
    "express-session": "^1.17.0",
    "express-validator": "^6.4.0",
    "if-env": "^1.0.4",
    "mongodb": "^3.5.2",
    "mongoose": "^5.8.11",
    "sanitize": "^2.1.0"
  },
  "bugs": {
    "url": "https://github.com/shodges201/healthy-living/issues"
  },
  "homepage": "https://github.com/shodges201/healthy-living#readme"
}
